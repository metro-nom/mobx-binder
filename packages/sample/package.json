{
    "name": "sample",
    "version": "0.3.1",
    "private": true,
    "description": "A frontend boilerplate with React, MobX and Typescript",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --max-warnings 0 'src/**/*.ts*'",
        "lint:fix": "npm run lint -- --fix",
        "start": "webpack-dev-server --mode development --hot --progress --colors --port 3000 --open",
        "build": "mkdir -p dist && webpack -p --progress --colors",
        "create-stats": "NODE_ENV=production webpack --profile --json > webpack-stats.json",
        "show-stats": "webpack-bundle-analyzer webpack-stats.json",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
    },
    "license": "MIT",
    "dependencies": {
        "bootstrap": "^4.1.3",
        "classnames": "^2.2.6",
        "mobx": "^4.0.0 || ^5.0.0",
        "mobx-binder": "^0.3.1",
        "mobx-binder-moment": "^0.3.1",
        "mobx-react": "^5.0.0",
        "moment": ">=2.0.0",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-mobx-i18n": "^1.1.0",
        "reactstrap": "^6.4.0",
        "tslint": "^5.12.1"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/classnames": "^2.2.6",
        "@types/mocha": "^5.2.6",
        "@types/node": "^9.6.32",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.8",
        "@types/react-router": "^4.0.31",
        "@types/reactstrap": "^6.4.0",
        "@types/webpack": "^3.8.14",
        "chai": "^4.2.0",
        "css-loader": "^0.28.11",
        "file-loader": "^1.1.11",
        "html-loader": "^1.0.0-alpha.0",
        "html-webpack-plugin": "^3.2.0",
        "mobx-react-devtools": "^4.2.15",
        "mocha": "^6.0.1",
        "style-loader": "^0.20.3",
        "ts-loader": "^4.5.0",
        "typescript": "^3.0.3",
        "url-loader": "^1.1.1",
        "webpack": "4.19.1",
        "webpack-bundle-analyzer": "^3.0.4",
        "webpack-cleanup-plugin": "^0.5.1",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.0",
        "webpack-hot-middleware": "^2.24.2"
    }
}
